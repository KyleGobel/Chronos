<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">  
  <metadata>
    <id>Chronos.PostgreSQL</id>
    <version>0.0.0</version>
    <authors>Kyle Gobel</authors>
    <description>Postgres extension to Chronos, contains a postgres implementation of the bulk insert</description>
    <language>en-US</language>
    <projectUrl>https://github.com/KyleGobel/Chronos/</projectUrl>
    <dependencies>
      <dependency id="Chronos" />
      <dependency id="Npgsql" />
    </dependencies>
  </metadata>
 
  <files>
    <file src="..\..\bin\Chronos.PostgreSQL.dll" target="lib"/>
    <file src="..\..\bin\Chronos.PostgreSQL.pdb" target="lib"/>
    <file src="..\..\src\Chronos.PostgreSQL\**\*.cs" target="src" />
  </files>
</package>